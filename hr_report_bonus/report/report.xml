<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="paper_format_bonus_report" model="report.paperformat">
        <field name="name">Bonus Report A4</field>
        <field name="default" eval="True"/>
        <field name="format">A4</field>
        <field name="page_height">0</field>
        <field name="page_width">0</field>
        <field name="orientation">Portrait</field>
        <field name="margin_top">35</field>
        <field name="margin_bottom">30</field>
        <field name="margin_left">5</field>
        <field name="margin_right">5</field>
        <field name="header_line" eval="True"/>
        <field name="header_spacing">30</field>
        <field name="dpi">100</field>
    </record>

    <report
            id="bonus_report_pdf"
            string="Bonus"
            model="bonus.report"
            report_type="qweb-pdf"
            name="hr_report_bonus.bonus_report_id_print"
            file="hr_report_bonus.bonus_report_id_print"
            paperformat="paper_format_bonus_report"/>
</odoo>